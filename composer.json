{
    "name": "skyraptor/laravel-fullcalendar",
    "description": "Laravel helper for FullCalendar.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Shawn Tunney",
            "email": "shawn.tunney@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3 | ^8.0",
        "ext-json": "*",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16.1",
        "orchestra/testbench": "^4.6|^5.0",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "MaddHatter\\LaravelFullcalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaddHatter\\LaravelFullcalendar\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MaddHatter\\LaravelFullcalendar\\Providers\\FullCalendarServiceProvider"
            ],
            "aliases": {
                "Calendar": "MaddHatter\\LaravelFullcalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "stable"
}
